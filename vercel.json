{
    "version": 2,
    "functions": {
      "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://yourproductionurl.com",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
    }
  }
